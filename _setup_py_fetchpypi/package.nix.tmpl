{{ buildPythonPackage, fetchPypi, lib, setuptools{native_build_inputs_declaration}{propagated_build_inputs_declaration}{build_inputs_declaration} }}:
buildPythonPackage rec {{
  pname = "{package_name}";
  version = "{package_version}";
  format = "pyproject";

  src = fetchPypi {{
    inherit pname version;
    sha256 = "{pypi_sha256}";
  }};

  propagatedBuildInputs = [
    {propagated_build_inputs_with_newlines}
  ];

  checkInputs = [
    {check_inputs_with_newlines}
  ];

  meta = with lib; {{
    description = ''
